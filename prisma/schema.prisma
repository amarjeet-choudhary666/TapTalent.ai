generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql" // or "sqlite" for testing
  url      = env("DATABASE_URL")
}

model Quote {
  id          Int       @id @default(autoincrement())
  source      String     // URL of the quote source
  buyPrice    Float
  sellPrice   Float
  currency    String  
  fetchedAt   DateTime   @default(now())
}

